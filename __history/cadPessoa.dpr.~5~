program cadPessoa;

uses
  Vcl.Forms,
  unitPrincipal in 'unitPrincipal.pas' {Form2},
  unitDM in 'DM\unitDM.pas' {DM: TDataModule},
  unitCadastroPessoa in 'Cadastros\unitCadastroPessoa.pas' {frmCadastroPessoa},
  utils in 'Utils\utils.pas';

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TForm2, Form2);
  Application.CreateForm(TDM, DM);
  Application.CreateForm(TfrmCadastroPessoa, frmCadastroPessoa);
  Application.Run;
end.
